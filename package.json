{
  "name": "create-react-package-esbuild",
  "version": "1.0.3",
  "description": "Tool to export component package to npm with preview",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "module": "dist/index.js",
  "scripts": {
    "dev": "es-dev-server",
    "start": "npx nodemon --config nodemon.json",
    "reloadBundle": "node esBuildReload.config.js --watch",
    "build": "npx tsc --project tsconfig.build.json && node esbuild.config.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "workingWatcher": "npx esbuild src/Preview.tsx --bundle --outdir=public/js --watch --servedir=public"
  },
  "keywords": [],
  "author": "de4dp1xl",
  "license": "ISC",
  "dependencies": {
    "@react-spring/web": "^9.7.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.9"
  },
  "devDependencies": {
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "@types/styled-components": "^5.1.26",
    "axios": "^1.3.5",
    "es-dev-server": "^2.1.0",
    "esbuild": "^0.17.16",
    "eslint": "^8.38.0",
    "nodemon": "^2.0.22",
    "puppeteer-core": "^19.9.1",
    "serve": "^14.2.0",
    "socket.io": "^4.6.1",
    "typescript": "^5.0.4"
  }
}
