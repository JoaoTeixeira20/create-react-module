{
  "name": "create-react-package-esbuild",
  "version": "1.0.5",
  "description": "Tool to export component package to npm with preview",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "module": "dist/index.js",
  "scripts": {
    "start": "node client-env-config.js && concurrently \"npm run start:react-preview\" \"npm run start:hot-reload-server",
    "start:react-preview": "node esbuildwatch.config.js",
    "start:hot-reload-server": "node sse-hot-reload-handler/sse-server.js",
    "build": "npx tsc --project tsconfig.build.json && node esbuild.config.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "de4dp1xl",
  "license": "ISC",
  "dependencies": {
    "@react-spring/web": "^9.7.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.9"
  },
  "devDependencies": {
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "@types/styled-components": "^5.1.26",
    "esbuild": "^0.17.16",
    "eslint": "^8.38.0",
    "typescript": "^5.0.4",
    "concurrently": "^8.0.1",
    "dotenv": "^16.0.3"
  }
}
